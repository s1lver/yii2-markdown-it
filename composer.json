{
  "name": "s1lver/yii2-markdown-it",
  "type": "yii2-extension",
  "minimum-stability": "stable",
  "require": {
    "php": "^8.0|^8.1",
    "yiisoft/yii2": "^2.0.43"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "s1lver\\widgets\\": "src"
    }
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  }
}